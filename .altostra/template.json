          "aws": {
            "billingModel": "PROVISIONED",
            "throughput": {
              "readUnits": 5,
              "writeUnits": 5
            }
          },
          "id": "Data01",
          "key": {
            "hash": {
              "name": "pk",
              "type": "STRING"
            },
            "range": {
              "name": "sk",
              "type": "STRING"
            }
          },
          "name": "data",
          "tableName": "data",
          "type": "resource.table"
        },
        "HandleResources01": {
          "entryPoint": "functions/handleResources.handler",
          "id": "HandleResources01",
          "layers": [],
          "memory": 512,
          "name": "handle-resource",
          "runtime": "nodejs12.x",
          "timeout": 6,
          "type": "resource.compute.function"
        }
      },
      "unsafe": false,
      "version": "2019-10-24"
    }
